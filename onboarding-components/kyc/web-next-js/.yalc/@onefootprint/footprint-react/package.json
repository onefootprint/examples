    "lodash": "^4.17.21",
    "react-hook-form": "^7.52.2"
  },
  "peerDependencies": {
    "@onefootprint/footprint-js": ">=3.15.0",
    "react": "17 - 18",
    "react-dom": "17 - 18"
  },
  "publishConfig": {
    "access": "public"
  },
